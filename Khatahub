local Window = MakeWindow({
    Hub = {
        Title = "khata hub",
        Animation = "khata"
    },
    Key = {
        KeySystem = false,
        Title = "Key System",
        Description = "1+1+2×2+6+4=key",
        KeyLink = "",
        Keys = {"16"},
        Notifi = {
            Notifications = true,
            CorrectKey = "Running the Script...",
            Incorrectkey = "The key is incorrect",
            CopyKeyLink = "Copied to Clipboard"
        }
    }
})

